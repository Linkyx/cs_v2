<!DOCTYPE html>
<html>
<head>
  <meta charset="utf8">
  <title>{{title}}</title>
  <link rel="stylesheet" href="//cdn.bootcss.com/bootstrap/3.3.5/css/bootstrap.min.css">
  <link rel="stylesheet" href="//cdn.bootcss.com/font-awesome/4.5.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="/assets/css/shared/ui.css">
  <link rel="stylesheet" href="/assets/css/shared/layout.css">
  {{block ('headSection')}}
</head>
<body class="app">
  <header class="navbar navbar-fixed-top {{#if (cookies.collapsed_nav)}}header-collapsed{{else}}header-expanded{{/if}}">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="#">Wiki</a>
      </div>

      <div class="collapse navbar-collapse">
        <ul class="nav navbar-nav navbar-right">
          <li>
            <form class="navbar-form" role="search" action="/search">
              <div class="form-group">
                <input type="text" class="form-control" placeholder="Search">
              </div>
            </form>
          </li>
          <li>
            <a href="#" data-toggle="tooltip" data-placement="bottom" data-original-title="我的消息">
              <span class="badge">1</span>
            </a>
          </li>
          <li>
            <a href="repositorys/new" data-toggle="tooltip" data-placement="bottom" data-original-title="新建仓库">
              <i class="fa fa-plus"></i>
            </a>
          </li>
          <li>
            <a href="#" data-toggle="tooltip" data-placement="bottom" data-original-title="退出">
              <i class="fa fa-sign-out"></i>
            </a>
          </li>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </header>
  <div class="page-with-sidebar {{#if (cookies.collapsed_nav)}}page-sidebar-collapsed{{else}}page-sidebar-expanded{{/if}}">
    <aside class="sidebar {{#if (cookies.collapsed_nav)}}sidebar-collapsed{{else}}sidebar-expanded{{/if}}">
      <a class="header-logo" href="#">
        <i class="fa fa-tv"></i>
        <span class="app-name">西邮Linux</span>
      </a>
      <ul class="nav sidebar-nav">
        <li class="active">
          <a title="" href="/repositorys" data-original-title="文档列表">
            <i class="fa fa-home"></i>
            <span>文档列表</span>
          </a>
        </li>
        <li>
          <a title="" href="/dashboard/projects" data-original-title="Projects">
            <i class="fa fa-home"></i>
            <span>Projects</span>
          </a>
        </li>
        <li>
          <a title="" href="/dashboard/projects" data-original-title="Projects">
            <i class="fa fa-home"></i>
            <span>Projects</span>
          </a>
        </li>
      </ul>
      <a class="nav-collapse-toggle" title="Open/Close" href="#">
        {{#if (cookies.collapsed_nav)}}
          <i class="fa fa-angle-right"></i>
        {{else}}
          <i class="fa fa-angle-left"></i>
        {{/if}}
      </a>
    </aside>
    <div class="page-content">
      {{block ('body')}}
    </div>
  </div>
  <script src="/assets/libs/jquery/jquery.js"></script>
  <script src="/assets/libs/bootstrap/bootstrap.js"></script>
  <script src="/assets/libs/seajs/sea.js"></script>
  <script src="/assets/libs/seajs/sea-config.js"></script>
  <script>
    seajs.use(['js/shared/polyfills', 'js/shared/layout']);
  </script>
  {{block ('footSection')}}
</body>
</html>
